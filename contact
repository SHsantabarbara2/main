                var url = event.target.id + ".html";
                $("#content").load(url);
                location.hash = event.target.id;
            });
        });

        window.onhashchange = function(){
            var what_to_do = location.hash.substring(1) + ".html";  
            $("#content").load(what_to_do);
        }

